#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/tomcrown/poll_system/sui-voting-frontend/node_modules/.pnpm/@wallet-standard+errors@0.1.1/node_modules/@wallet-standard/errors/bin/node_modules:/home/tomcrown/poll_system/sui-voting-frontend/node_modules/.pnpm/@wallet-standard+errors@0.1.1/node_modules/@wallet-standard/errors/node_modules:/home/tomcrown/poll_system/sui-voting-frontend/node_modules/.pnpm/@wallet-standard+errors@0.1.1/node_modules/@wallet-standard/node_modules:/home/tomcrown/poll_system/sui-voting-frontend/node_modules/.pnpm/@wallet-standard+errors@0.1.1/node_modules:/home/tomcrown/poll_system/sui-voting-frontend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/tomcrown/poll_system/sui-voting-frontend/node_modules/.pnpm/@wallet-standard+errors@0.1.1/node_modules/@wallet-standard/errors/bin/node_modules:/home/tomcrown/poll_system/sui-voting-frontend/node_modules/.pnpm/@wallet-standard+errors@0.1.1/node_modules/@wallet-standard/errors/node_modules:/home/tomcrown/poll_system/sui-voting-frontend/node_modules/.pnpm/@wallet-standard+errors@0.1.1/node_modules/@wallet-standard/node_modules:/home/tomcrown/poll_system/sui-voting-frontend/node_modules/.pnpm/@wallet-standard+errors@0.1.1/node_modules:/home/tomcrown/poll_system/sui-voting-frontend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@wallet-standard/errors/bin/cli.mjs" "$@"
else
  exec node  "$basedir/../@wallet-standard/errors/bin/cli.mjs" "$@"
fi
